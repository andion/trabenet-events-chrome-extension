{
  "name": "Trabenet events",
  "version": "1.5",
  "update_url": "http://github.com/andion/trabenet-events-chrome-extension/raw/master/updates.xml"
  "background_page": "background.html",
  "description": "Get Trabe intranet event's right on your browser.",
  "icons": {
    "128" : "icon_128.png"
  },
  "browser_action": {
    "default_icon": "trabe_not_logged_in.png",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://events.trabesoluciones.net/*"
  ]
}

